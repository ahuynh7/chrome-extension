{
    "name": "Swear",
    "description": "mvp build for sustainable clothing extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "/icons/icon-16.png",
        "32": "/icons/icon-32.png",
        "48": "/icons/icon-64.png",
        "128": "/icons/icon-128.png"
    },
    "action": {
        "default_popup": "popup.html"
    }
}